generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id        Int     @id @default(autoincrement())
  name      String
  email     String
  lastname  String
  password  String
  published Boolean @default(false)
}

model Images {
  id        Int      @id @default(autoincrement())
  published Boolean  @default(false)
  authorId  Int
  createdAt DateTime @default(now())
  title     String
}

model Videos {
  id        Int      @id @default(autoincrement())
  published Boolean  @default(false)
  authorId  Int
  imageName String
  createdAt DateTime @default(now())
  title     String
}
